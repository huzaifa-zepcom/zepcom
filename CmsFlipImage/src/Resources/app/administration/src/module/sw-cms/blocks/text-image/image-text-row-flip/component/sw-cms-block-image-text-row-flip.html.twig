{% block sw_cms_block_image_text_row_flip %}
<div class="sw-cms-block-image-text-row-flip">
    <div class="sw-cms-block-image-text-row-flip__left">
        <slot name="left-image"></slot>
        <slot name="left-image-flip"></slot>
        <slot name="left-text"></slot>
    </div>

    <div class="sw-cms-block-image-text-row-flip__center">
        <slot name="center-image"></slot>
        <slot name="center-image-flip"></slot>
        <slot name="center-text"></slot>
    </div>

    <div class="sw-cms-block-image-text-row-flip__right">
        <slot name="right-image"></slot>
        <slot name="right-image-flip"></slot>
        <slot name="right-text"></slot>
    </div>
</div>
{% endblock %}
